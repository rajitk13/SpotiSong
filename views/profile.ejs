<%- include('partials/header'); -%>
    <section id='home'>
        <h1>Profile Info</h1>
        <div class="container">
            <div class="row">
                <div class="col-sm">
                    <h3>
                        <%=body.display_name%>
                    </h3>
                </div>
            </div>
            <div class="row">
                <div class="col-sm">
                    <h3>
                        <%=body.email%>
                    </h3>
                </div>
                <div class="col-sm-12 col-lg-6 col-md-6">
                    <h3>Total Followers: 
                        <%=body.followers.total%>
                    </h3>
                </div>
            </div>
            <div class="row">
                <div class="col-sm">
                    <h3>
                        <a href="<%=body.external_urls.spotify%>">
                            <%=body.external_urls.spotify%>
                        </a>
                    </h3>

                </div>
            </div>

        </div>
    </section>


    <%- include('partials/footer'); -%>